{
  "version": 2,
  "functions": {
    "api/index.py": {
      "runtime": "@vercel/python@3.1.0"
    },
    "api/[path].py": {
      "runtime": "@vercel/python@3.1.0"
    }
  },
  "routes": [
    {
      "src": "^/(.*)",
      "dest": "/api/[path].py"
    },
    {
      "src": "/",
      "dest": "/api/index.py"
    }
  ]
}